<ul>
  <% @photos.each do |photo| %>
  <li>
    <% photo.title %>
    <%= link_to 'Show', photo_path(photo) %>
    <%= link_to 'Edit', edit_photo_path(photo) %>
    <%= link_to 'Destroy', photo_path(photo), method: :delete, date:{confirm: "確定刪除嘛??"} %>
  </li>
  <% end %>
</ul>

<%= link_to 'New Photo', new_photo_path %>